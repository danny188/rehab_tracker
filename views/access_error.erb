


<div class="col-5 text-center alert-warning mx-auto">
  <h5>
  <% if session[:access_error] %>
    <%= session.delete(:access_error) %>
  <% else %>
    Sorry, you do not have access to the requested page, or your session has timed out. Please sign in again.
  <% end %>
  </h5>
</div>

<br>

<div class="text-center">
<h5>Go to <a class="btn btn-primary" href="/login">Sign In</a></h5>

</div>